<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="405.0" prefWidth="338.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.gui_javafx.EnergyGuiController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="communityLabel" prefHeight="26.0" prefWidth="290.0" text="Community Pool" />
      <Label fx:id="gridLabel" prefHeight="22.0" prefWidth="286.0" text="Grid Portion" />
      <Button fx:id="refreshButton" mnemonicParsing="false" prefHeight="26.0" prefWidth="81.0" text="refresh" />
      <HBox prefHeight="39.0" prefWidth="298.0">
         <children>
            <Label prefHeight="28.0" prefWidth="126.0" text="Start" />
            <DatePicker fx:id="startDate" />
         </children>
      </HBox>
      <HBox prefHeight="33.0" prefWidth="298.0">
         <children>
            <Label prefHeight="18.0" prefWidth="126.0" text="End" />
            <DatePicker fx:id="endDate" />
         </children>
      </HBox>
      <Button fx:id="showDataButton" mnemonicParsing="false" text="show Data" />
      <TextArea fx:id="outputArea" prefHeight="103.0" prefWidth="298.0" />
   </children>
</VBox>
